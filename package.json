{
  "name": "bicycle",
  "private": true,
  "version": "9.1.1",
  "description": "A data synchronisation library for JavaScript",
  "keywords": [],
  "dependencies": {
    "@authentication/lock-by-id": "^0.0.1",
    "@types/body-parser": "^1.16.4",
    "@types/cuid": "^1.3.0",
    "@types/deep-freeze": "^0.1.1",
    "@types/express": "^4.0.36",
    "@types/leven": "^2.1.0",
    "@types/ms": "^0.7.29",
    "@types/node": "^8.0.16",
    "body-parser": "^1.15.2",
    "character-parser": "^3.0.0",
    "cuid": "^2.0.2",
    "deep-freeze": "0.0.1",
    "leven": "^2.0.0",
    "ms": "^1.0.0",
    "promise": "^8.0.1",
    "stable-stringify": "^1.0.0",
    "then-request": "^4.1.0",
    "throat": "^3.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7",
    "@microsoft/tsdoc": "^0.12.5",
    "@types/get-port": "0.0.4",
    "@types/jest": "*",
    "@types/rimraf": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-register": "^6.24.1",
    "babelify": "*",
    "browserify-middleware": "*",
    "express": "*",
    "get-port": "*",
    "husky": "*",
    "jest": "*",
    "lint-staged": "*",
    "lsr": "*",
    "opaque-types": "^1.1.2",
    "prettier": "*",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "todomvc-app-css": "^2.0.3",
    "todomvc-common": "^1.0.2",
    "ts-jest": "*",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "prebuild": "rimraf lib && opaque-types",
    "build": "tsc",
    "postbuild": "node lib/scripts/prepare-docs && rimraf lib/scripts && node lib/build",
    "pretest": "yarn build",
    "test": "jest ./src --coverage",
    "watch": "jest ./src --coverage --watch",
    "prerelease": "yarn prettier && yarn build",
    "release": "cd lib && yarn publish",
    "prestart": "yarn build",
    "start": "node example"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicyclejs/bicycle.git"
  },
  "author": "ForbesLindesay",
  "license": "MIT"
}
